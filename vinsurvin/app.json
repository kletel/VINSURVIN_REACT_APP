{
  "expo": {
    "name": "vinsurvin",
    "slug": "vinsurvin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/newLogo.png",
    "scheme": "vinsurvin",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "fr.vitissia.vinsurvin",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "CFBundleDevelopmentRegion": "fr",
        "NSCameraUsageDescription": "Cette application utilise la caméra pour prendre des photos de vos bouteilles.",
        "NSPhotoLibraryUsageDescription": "Cette application accède à votre bibliothèque pour sélectionner des photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./public/vitissia_LOGO.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "fr.vitissia.vinsurvin",
       "permissions": [
        "CAMERA",
        "READ_MEDIA_IMAGES"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./public/newLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d0ae4491-7b4c-4a1a-b05c-de9ae7f77967"
      }
    }
  }
}